{
  "name": "backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "postinstall": "npm run prisma:generate",
    "prebuild": "rimraf dist && npm run prisma:generate",
    "build": "nest build",
    "format": "eslint {src,test}/**/*.ts --fix",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "start": "nest start",
    "start:dev": "USE_INTERNAL_DEVELOPMENT_DOCKER_DB=true NODE_ENV=development nest start --watch",
    "start:test": "USE_INTERNAL_TEST_DOCKER_DB=true nest start --watch",
    "test": "TZ=Europe/Helsinki jest",
    "test:watch": "npm run test -- --watch",
    "test:cov": "npm run test -- --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm run test --  --config ./test/jest-e2e.json",
    "types:export": "NEST_ONLY_EXPORT_API_SPEC=TRUE NODE_ENV=development npm run start && prettier --write ./api-spec.json",
    "check-types": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:format": "prisma format",
    "prisma:push": "prisma db push",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org silte --project financer-backend ./dist && sentry-cli --url https://sentry.silte.fi/ sourcemaps upload --org silte --project financer-backend ./dist"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.8",
    "@nestjs/config": "^4.0.0",
    "@nestjs/core": "^11.0.8",
    "@nestjs/mapped-types": "^2.1.0",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.8",
    "@nestjs/serve-static": "^5.0.2",
    "@nestjs/swagger": "^11.0.3",
    "@prisma/client": "^6.3.1",
    "@sentry/cli": "^2.41.1",
    "@sentry/nestjs": "^8.54.0",
    "@sentry/profiling-node": "^8.54.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "connect-mongo": "^5.1.0",
    "connect-pg-simple": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "date-fns-tz": "^3.2.0",
    "eslint-plugin-unused-imports": "^4.1.4",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "mongodb-memory-server": "^10.1.3",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "passport-github2": "^0.1.12",
    "pg": "^8.13.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^11.0.2",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.8",
    "@types/connect-pg-simple": "^7.0.3",
    "@types/cookie-parser": "^1.4.8",
    "@types/express": "^5.0.0",
    "@types/express-session": "^1.18.1",
    "@types/jest": "29.5.14",
    "@types/node": "^22.13.1",
    "@types/passport": "^1.0.17",
    "@types/passport-auth0": "^1.0.9",
    "@types/passport-github2": "^1.2.9",
    "@types/pg": "^8.11.11",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.23.0",
    "@typescript-eslint/parser": "^8.23.0",
    "eslint": "^9.19.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.3.1",
    "rimraf": "^6.0.1",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.23.0"
  }
}
