    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "mongodb-memory-server": "^9.1.6",
    "passport": "^0.7.0",
    "passport-auth0": "^1.4.4",
    "passport-github2": "^0.1.12",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1"
  },
