{
  "name": "financer",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "start": "pnpm run --parallel --stream start:dev",
    "start:ci": "NODE_ENV=test SCHEMA=packages/backend/prisma/schema.prisma PORT=3000 node build/server/main.js",
    "start:test": "pnpm run --parallel --stream start:test",
    "start:test-internal": "NODE_ENV=test SCHEMA=packages/backend/prisma/schema.prisma node build/server/main.js",
    "lint": "pnpm --parallel --stream  lint",
    "format": "pnpm --parallel --stream format",
    "test": "CI=true pnpm --parallel --stream test",
    "build": "./bin/buildApp",
    "e2e:test": "pnpm --filter @local/playwright run test --",
    "e2e:open": "pnpm --filter @local/playwright run ui --",
    "types:update": "pnpm --filter backend run types:export && pnpm run types:copy && pnpm run types:generate",
    "types:copy": "cp packages/backend/api-spec.json packages/frontend/src/redux/schema/api-spec.json && cp packages/backend/api-spec.json packages/playwright/types/api-spec.json",
    "types:generate": "pnpm  --parallel --stream types:generate",
    "check-types": "pnpm --parallel --stream check-types"
  },
  "author": "",
