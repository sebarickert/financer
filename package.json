{
  "name": "financer",
  "version": "1.0.0",
  "description": "",
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "start": "run-p start:backend start:frontend",
    "start:ci": "run-p start:backend:ci start:frontend",
    "start:test": "run-p start:backend:test start:frontend",
    "start:backend": "npm -w backend run start:dev",
    "start:backend:ci": "NODE_ENV=test npm -w backend run start",
    "start:backend:test": "NODE_ENV=test npm -w backend run start-dev",
    "start:frontend": "npm -w frontend run start",
    "lint": "npm -ws run lint",
    "format": "npm -ws run format",
    "test": "CI=true npm -w backend run test && CI=true npm -w frontend run test",
    "build": "./bin/buildApp",
    "cypress:run": "npm -w cypress run cypress:run",
    "cypress:open": "npm -w cypress run cypress:open"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
